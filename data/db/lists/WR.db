/set WRsign=@{BxCyellow}(@{BxCred}~WR~@{BxCyellow})@{n}
/def _WR_player_sub = \
    /substitute -p @{}%{PL}%{P1}%{WRsign}%{P4- }%{PR}%;\
            /echo 1: %{P1}%;\
    /echo 2: %{P2}%;\
    /echo 3: %{P3}%;\
    /echo 4: %{P4}%;\
    /echo 5: %{P5}%;\
    /if ({P4}=~"." | {P4}=~",")         /let target=$(/_odmien_M_B %{P1})%;\
        /eval /def key_f6 = /zabij %{target}%;\
    /endif

/def -Fp3 -mregexp -t'((c|C)zarnowlosy czarnooki (?:mutant|mutantka|mezczyzna)|Arid)([*,.\] ]|$)' _player_sub_231 = /_WR_player_sub
/def -Fp3 -mregexp -t'((p|P)onury brzuchaty (?:mutant|mutantka|ogr)|Maldun)([*,.\] ]|$)' _player_sub_232 = /_WR_player_sub
/def -Fp3 -mregexp -t'((z|Z)walisty bezzebny (?:mutant|mutantka|mezczyzna)|NieznanyZImienia)([*,.\] ]|$)' _player_sub_233 = /_WR_player_sub